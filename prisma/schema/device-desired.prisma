model DeviceDesired {
  id                  String   @id @default(uuid())
  spaceId             String   @unique @map("space_id")
  samplingIntervalSec Int      @default(10) @map("sampling_interval_sec")
  co2AlertThreshold   Int      @default(1000) @map("co2_alert_threshold")
  updatedAt           DateTime @updatedAt @map("updated_at")

  space Space @relation(fields: [spaceId], references: [id])

  @@map("device_desired")
}
